{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.js", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/middleware.js", "src/store/Provider.jsx", "src/store/store.js", "src/store/reducer/userFetchReducer.js", "src/app/(pages)/page.jsx", "src/app/(pages)/layout.jsx", "src/app/layout.jsx", "tailwind.config.js"],
  "exclude": ["node_modules"]
}
